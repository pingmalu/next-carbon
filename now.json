{
  "name": "carbon",
  "version": 2,
  "public": true,
  "routes": [
    {
      "src": "^/about(.*)",
      "dest": "/about"
    },
    {
      "src": "^/embed(.*)",
      "dest": "/embed"
    },
    {
      "src": "^/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "^/robots.txt",
      "dest": "/static/robots.txt"
    },
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/service-worker.js",
      "headers": {
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "github": {
    "autoAlias": false,
    "silent": true
  },
  "functions": {
    "api/*.php": {
      "runtime": "now-php@0.0.10",
      "memory": 3008,
      "maxDuration": 500
    }
  }
}